{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "build:dev": "ng build -ec && gulp manifest",
    "build:prod": "ng build --prod && gulp manifest",
    "manifest": "gulp manifest",
    "build:teamcity": "npm run lint && ng test --single-run --reporters coverage-istanbul,teamcity --code-coverage --browsers Chrome_ci --colors=false && ng build --prod && gulp manifest",
    "start": "ng build --base-href '/client/' && gulp manifest && ng build -w -ec",
    "lint": "ng lint && npm run stylelint",
    "stylelint": "stylelint src/**/*.scss --syntax scss",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.1",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "cheerio": "^0.22.0",
    "codelyzer": "~2.0.0",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-asset-manifest": "0.0.6",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-teamcity-reporter": "^1.0.0",
    "merge-stream": "^1.0.0",
    "protractor": "~5.1.0",
    "stylelint": "^7.10.1",
    "stylelint-scss": "^1.4.4",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
